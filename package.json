{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "local": "nuxt prepare && nuxt dev --dotenv .env.local",
    "dev": "nuxt prepare && nuxt dev --dotenv .env.development",
    "production": "nuxt dev --dotenv .env.production",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.11",
    "axios": "^1.4.0",
    "cookie-signature": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "ioredis": "^5.3.2",
    "lint-staged": "^13.2.2",
    "pinia": "^2.1.4",
    "prettier": "^2.8.8",
    "skyway-js": "^4.4.5",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.6.7",
    "@sidebase/nuxt-auth": "^0.6.0-beta.3",
    "@sidebase/nuxt-session": "^0.2.7",
    "@types/cookie-signature": "^1.1.0",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.11.0",
    "nuxt": "^3.4.3",
    "postcss-html": "^1.5.0",
    "sass": "^1.62.0",
    "sass-loader": "^10",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0",
    "typescript": "^5.0.4"
  }
}
